rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // Receipt images - only family members can read/write
    match /receipts/{familyGroupId}/{allPaths=**} {
      allow read, write: if request.auth != null;
    }

    // Default deny all
    match /{allPaths=**} {
      allow read, write: if false;
    }
  }
}
